{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "first_name": {
        "type": "string"
      },
      "last_name": {
        "type": "string"
      },
      "address": {
        "type": "object",
        "properties": {
          "lines": {
            "type": "array",
            "items": [
              {
                "type": "string"
              }
            ]
          },
          "postcode": {
            "type": "string"
          }
        },
        "required": [
          "lines",
          "postcode"
        ]
      }
    },
    "required": [
      "first_name",
      "last_name",
      "address"
    ]
  }